{
    "version": "0.0.0.2",
    "manifest_version": 3,
    "author": "Brave Sampson",
    "name": "Facebook Sponsor Blocker",
    "description": "Block sponsored content on Facebook",
    "content_scripts": [
        {
            "matches": ["*://*.facebook.com/*"],
            "run_at": "document_start",
            "js": ["blocker.js"]
        }
    ]
}